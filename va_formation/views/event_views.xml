<odoo>
  <data>
    
    <record model="ir.ui.view" id="view_event_type_form">
        <field name="name">event.type.form</field>
        <field name="model">event.type</field>
        <field name="inherit_id" ref="event.view_event_type_form"/>
        <field name="arch" type="xml">

        <div class="oe_title" position="after">
            <h2>Word Press Info</h2>
            <group>
              <group>
                  <field name="short_name"/>
                  <field name="category"/>
                  <field name="parent_id"/>
                  <field name="doc_url" widget="url"/>
                  <field name="subtitle"/>
                  <field name="description"/>
                  <field name="responsible_id"/>
                  <field name="responsible_info"/>
                  <field name="price_single"/>
                  <field name="price_group"/>
              </group>
              <group>
                  <field name="is_published" widget='boolean_toggle'/>
                  <field name="image" widget="image"/>
                  <field name="objectives"/>
                  <field name="requirements"/>
              </group>
            </group>
        </div> 
            
        </field>
    </record>

    <record model="ir.ui.view" id="view_event_form">
        <field name="name">event.form</field>
        <field name="model">event.event</field>
        <field name="inherit_id" ref="event.view_event_form"/>
        <field name="arch" type="xml">

          <field name="event_type_id" position="after">
            <field name="short_name"/>
            <field name="reporting_event_type_id"/>
            <field name="is_private"/>
          </field>

        </field>
    </record>

  </data>
</odoo>