

<odoo>
   <data>
       <template id="layout" name="Event" inherit_id="website_event.layout" >
           <xpath expr="//div[@id='wrap']" position="replace">
            <div id="wrap" class="o_wevent_event js_event">
                <t t-raw="0"/>
