<odoo>
  <data>

    <record model="ir.actions.act_window" id="action_safety_element">
      <field name="name">Audit Elements</field>
      <field name="res_model">safety.audit.element</field>
      <field name="view_mode">tree</field>
    </record>

    <menuitem
            id="menu_safety_config"
            name="Configuration"
            parent="menu_safety_root"
            sequence="12"
    />

    <menuitem
            id="menu_safety_element"
            name="Audit Element"
            parent="menu_safety_config"
            sequence="3"
            action="action_safety_element"
    />

    <record model="ir.ui.view" id="safety_element_list">
      <field name="name">safety_element_list</field>
      <field name="model">safety.audit.element</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
          <field name="description"/>
        </tree>
      </field>
    </record>

  </data>
</odoo>